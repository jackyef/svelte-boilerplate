{
  "name": "svelte-boilerplate",
  "version": "0.0.1",
  "description": "Svelte boilerplate using webpack",
  "main": "index.js",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-async-generators": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-destructuring": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/runtime": "^7.2.0",
    "app-root-dir": "^1.0.2",
    "babel-plugin-macros": "^2.4.4",
    "better-npm-run": "^0.1.1",
    "debug": "^4.1.1",
    "fs-extra": "^7.0.1"
  },
  "devDependencies": {
    "@tivac/eslint-plugin-svelte": "^1.2.1",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.0",
    "eslint-plugin-html": "^5.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "svelte": "^2.16.0",
    "svelte-loader": "^2.12.0",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.14"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:client": "bnr build:client",
    "dev:client": "bnr dev:client"
  },
  "betterScripts": {
    "build:client": {
      "command": "babel-node --max-old-space-size=4096 ./scripts/run build",
      "env": {
        "DEBUG": "build*",
        "DIR": "client",
        "NODE_ENV": "production"
      }
    },
    "dev:client": {
      "command": "webpack --config ./client/webpack.config.js",
      "env": {
        "DEBUG": "build*",
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackyef/svelte-boilerplate.git"
  },
  "keywords": [
    "svelte",
    "javascript",
    "webpack",
    "boilerplate"
  ],
  "author": "Jacky Efendi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jackyef/svelte-boilerplate/issues"
  },
  "homepage": "https://github.com/jackyef/svelte-boilerplate#readme"
}
